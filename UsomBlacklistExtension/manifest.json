{
   "manifest_version": 3,
   "name": "USOM Blacklist Checker",
   "version": "1.0",
   "permissions": [
       "webNavigation",
       "activeTab",
       "scripting"
   ],
   "background": {
       "service_worker": "background.js"
   },
   "host_permissions": [
       "*://*/*"
   ],
   "icons": {
       "48": "icon.png"
   }
}
